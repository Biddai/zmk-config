/ {
    chosen{
        zephyr,display = &oled;
        zmk,kscan = &default_kscan;
    };

    default_kscan: kscan{
        compatible = "zmk,kscan-gpio-direct";
        label = "default_kscan";

        input-gpios = 
                    <&pro_micro 4 (GPIO_ACTIVE_LOW  | GPIO_PULL_UP)>;
    };
};